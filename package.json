{
  "name": "@zhiwww/talib",
  "version": "0.7.0",
  "description": "High-performance Node.js wrapper for TA-Lib (Technical Analysis Library)",
  "homepage": "https://github.com/zhiwww/talib#readme",
  "bugs": {
    "url": "https://github.com/zhiwww/talib/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhiwww/talib.git"
  },
  "license": "ISC",
  "author": "Wei Zhi",
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "gypfile": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "install": "node-gyp rebuild",
    "build": "node-gyp rebuild",
    "build:debug": "node-gyp rebuild --debug",
    "clean": "node-gyp clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint:fix": "eslint src/**/*.js test/**/*.js --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "ta-lib",
    "technical analysis",
    "trading",
    "finance",
    "stock",
    "forex",
    "cryptocurrency",
    "indicators",
    "moving average",
    "rsi",
    "macd",
    "bollinger",
    "wrapper",
    "native",
    "addon"
  ],
  "dependencies": {
    "node-addon-api": "^7.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "node-gyp": "^10.0.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "lib/",
    "src/",
    "binding.gyp",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
